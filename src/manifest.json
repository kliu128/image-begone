{
  "name": "Image Begone",
  "version": "2.0.1",
  "description": "Fav Bookmark Manager",
  "manifest_version": 2,
  "icons": {},
  "browser_action": {
    "default_icon": {},
    "default_title": "Fav",
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": [
      "background-script/index.ts"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "https://*.github.com/*"
  ]
}